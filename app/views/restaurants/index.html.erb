<%= render "navbar" %>


<h1> Restaurants list</h1>

<ul>
  <% @restaurants.each do |restaurant| %>
    <li>
      <%= restaurant.name %>
      <%= link_to 'See details', restaurant_path(restaurant) %>
      <% end %>
    </li>
</ul>

<%= link_to 'Add a new restaurant', new_restaurant_path, class: "btn btn-primary" %>
